{
	"name": "Gmail RightLabel",
	"version": "0.0.2.0",
	"manifest_version": 2,

	"description": "This extension moves labels of email subjects to the right side.",

	"permissions": [
		"https://mail.google.com/*"
	],

	"icons": {
		"128": "images/icon128.png"
	},

	"content_scripts": [{
		"matches": ["https://mail.google.com/*"],
		"css": ["style.css"],
		"run-at": "document_end"
	}]
}
